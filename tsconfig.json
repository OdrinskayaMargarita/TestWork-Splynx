{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "module": "ESNext",
    "strict": true,
    "jsx": "react-native",
    "allowJs": true,
    "baseUrl": "./",
    "paths": {
      "@ali/components/*": [
        "src/components/*"
      ],
      "@ali/types": [
        "types"
      ],
      "@ali/hooks/*": [
        "src/hooks/*"
      ],
      "@ali/helpers/*": [
        "src/helpers/*"
      ],
      "@ali/types/*": [
        "src/types/*"
      ],
      "@ali/images/*": [
        "assets/images/*"
      ]
    }
  },
  "include": ["src/**/*", "types/**/*",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
